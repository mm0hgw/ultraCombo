revCombnG_recursive <- function(x,n){
	k <- length(x)
	ch <- choose(n-1,k-1)
	out <- x[k] - x[k-1]
	i <- 1
	j <- 1
	while(k-i>0){
		if(x[i]-j+1 == 1){
			ch <- ch*(k-i)/(n-j)
			i <- i + 1
			j <- j + 1
		}else{
			out <- out + ch
			ch <- ch - ch*(k-i)/(n-j)
			j <- j + 1
		}
	}
	out
}

buildPackage::buildPackage('ultraCombo',check=1)

x <- 1
n <- 100
k <- 3
ch <- choose(n,k)

while(x <= ch){
	y<-print(revCombnG(print(combnG(x,n,k)),n))
	stopifnot(x==y)
	x <- x + 1
}
